<%=form_with model: @posts do |f|%>
    <%=f.label :title%><br>
    <%=f.text_field :title%><br>
    <%=f.label :content%><br>
    <%=f.text_field :content%><br>
    <%=f.label :likes%><br>
    <%=f.number_field :likes%><br>
    <%=f.label :blogger_id%><br>
    <%=f.number_field :blogger_id%><br>
    <%=f.label :destination_id%><br>
    <%=f.number_field :destination_id%><br>

    <%=f.submit%>
    <%end%>
